    "lint": "eslint && prettier *.md **/*.md --check",
    "mdfix": "prettier *.md **/*.md --write",
    "precommit": "pnpm lint && pnpm test && pnpm build && git add *.md example/example.* LICENSE",
    "install_hooks": "husky"
