    "eslint-plugin-allowed-dependencies": "^1.3.1",
    "eslint-plugin-prettier": "^5.4.1",
    "husky": "^9.0.5",
    "prettier": "3.5.3",
    "tsup": "^8.5.0",
    "tsx": "^4.19.4",
    "typescript-eslint": "^8.34.0",
    "vitest": "^3.2.3"
  },
  "resolutions": {
    "@scarf/scarf": "npm:empty-npm-package@1.0.0"
