{
  "name": "@express-zod-api/zod-to-ts",
  "version": "2.0.0-beta.0",
  "license": "MIT",
  "description": "Zod-to-TS generator for express-zod-api",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobinTail/express-zod-api.git"
  },
  "author": {
    "name": "Anna Bocharova",
    "url": "https://robintail.cz",
    "email": "me@robintail.cz"
  },
  "bugs": "https://github.com/RobinTail/express-zod-api/issues",
  "funding": "https://github.com/sponsors/RobinTail",
  "scripts": {
    "pretest": "tsc --noEmit",
    "test": "vitest run",
    "build": "tsup",
    "postbuild": "attw --pack --profile esm-only",
    "prepublishOnly": "eslint && pnpm test && pnpm build"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "*.md"
  ],
  "dependencies": {
  },
  "peerDependencies": {
    "typescript": "^5.1.3",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "typescript": "catalog:dev",
    "zod": "catalog:dev"
  },
  "keywords": [
    "zod",
    "schema",
    "typescript",
    "generator"
  ]
}
